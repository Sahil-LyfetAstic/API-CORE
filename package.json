    "@types/cache-manager": "^4.0.2",
    "@types/compression": "^1.7.2",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/hbs": "^4.0.1",
    "@types/jest": "29.5.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "18.15.13",
    "@types/node-geocoder": "^4.2.1",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/pluralize": "^0.0.29",
    "@types/request-ip": "^0.0.37",
    "@types/sequelize": "^4.28.14",
    "@types/stripe": "^8.0.417",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "npm-run-all": "^4.1.5",
    "pm2": "^2.10.4",
    "prettier": "^2.8.7",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@core/mongo(|/.*)$": "<rootDir>/libs/mongo/src/$1",
      "^@core/sql(|/.*)$": "<rootDir>/libs/sql/src/$1",
      "^@core/email(|/.*)$": "<rootDir>/libs/email/src/$1",
      "^@core/firebase(|/.*)$": "<rootDir>/libs/firebase/src/$1",
      "^@core/geocoder(|/.*)$": "<rootDir>/libs/geocoder/src/$1",
      "^@core/recaptcha(|/.*)$": "<rootDir>/libs/recaptcha/src/$1",
      "^@core/twilio(|/.*)$": "<rootDir>/libs/twilio/src/$1",
      "^@core/stripe(|/.*)$": "<rootDir>/libs/stripe/src/$1"
    }
  }
}
